    "osm": {
        "name": "OpenStreetMap (beta)",
        "id": "osm",
        "notMapbox": true,
        "style": {
            "version": 8,
            "sources": {
                "osm": {
                    "type": "raster",
                    "tiles": [
                        "https://c.tile.openstreetmap.org/{z}/{x}/{y}.png"
                    ],
                    "tileSize": 256
                }
            },
            "layers": [
                {
                    "id": "osm",
                    "type": "raster",
                    "source": "osm"
                }
            ]
        }
    },
    "gmaps": {
        "name": "Google Maps (beta)",
        "id": "gmaps",
        "notMapbox": true,
        "style": {
            "version": 8,
            "sources": {
                "osm": {
                    "type": "raster",
                    "tiles": [
                        "https://mt1.google.com/vt/lyrs=m&x={x}&y={y}&z={z}"
                    ],
                    "tileSize": 256
                }
            },
            "layers": [
                {
                    "id": "gmaps",
